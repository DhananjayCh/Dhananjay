
@{
    ViewBag.Title = "Setting Page";
}

@section style
{
    <!-- Internal Select2 css -->
    <link href="~/assets/plugins/select2/css/select2.min.css" rel="stylesheet">

    <!---Internal Fancy uploader css-->
    <link href="~/Scripts/Angular/Intranet/JS/IntranetJs/fancyuploder/fancy_fileupload.css" rel="stylesheet" />

    <!-- Internal Data table css -->
    <link href="~/assets/plugins/datatable/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/assets/plugins/datatable/css/buttons.bootstrap4.min.css" rel="stylesheet">
    <link href="~/assets/plugins/datatable/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/assets/plugins/datatable/css/jquery.dataTables.min.css" rel="stylesheet">
    <link href="~/assets/plugins/datatable/css/responsive.dataTables.min.css" rel="stylesheet">
    <link href="~/assets/plugins/select2/css/select2.min.css" rel="stylesheet">

    <!---Internal Fileupload css-->
    <link href="~/Scripts/Angular/Intranet/JS/IntranetJs/fileuploads/css/fileupload.css" rel="stylesheet" type="text/css" />
    <!--Internal  Datetimepicker-slider css -->
    <link href="~/assets/plugins/amazeui-datetimepicker/css/amazeui.datetimepicker.css" rel="stylesheet">
    <link href="~/assets/plugins/sweet-alert/sweetalert.css" rel="stylesheet" />

}

@section styleIntranet
{

    <!--- CustomStyle css-->
    <link href="~/Scripts/Angular/Intranet/JS/IntranetJs/CustomStyle.css" rel="stylesheet">

    <link href="~/Scripts/Angular/Intranet/JS/IntranetJs/modelPopup.css" rel="stylesheet" />

    <!-- include summernote css/js -->
    <link href="~/Scripts/Angular/Intranet/JS/IntranetJs/summernote/css/summernote-bs4.min.css" rel="stylesheet">
}

<div class="container" ng-app="settingApp" ng-controller="settingController">
    <div class="breadcrumb-header justify-content-between">
        <div class="my-auto">
            <div class="d-flex">
                <h4 class="content-title mb-0 my-auto">Intranet</h4><span class="text-muted mt-1 tx-13 ml-2 mb-0">/ Setting</span>
            </div>
        </div>
    </div>
    <div class="row row-sm">
        <div class="col-lg-12 col-md-12">
            <div class="card" id="tabs-style4">
                <div class="card-body">
                    <div class="panel panel-primary">
                        <div class="tab_wrapper left_tab">
                            <ul class="tab_list tabClick">
                                <li class="active" data-title="Article">Article</li>
                                <li data-title="Notice">Notice</li>
                                <li data-title="Event">Event</li>
                                <li data-title="Slider">Slider</li>
                                <li data-title="Awards">Awards</li>
                                <li data-title="Photo Gallery">Photo Gallery</li>
                                <li data-title="Settings">Settings</li>
                            </ul>
                            <div class="content_wrapper">
                                <div class="tab_content active">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Article">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="ArticleDataTable" class="table key-buttons text-md-nowrap">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Article Title</th>
                                                            <th class="border-bottom-0">Description</th>
                                                            <th class="border-bottom-0">Pinned Article</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                        <tr ng-repeat="a in ArticleData">
                                                            <td>{{a.ID}}</td>
                                                            <td>{{a.Article_Title}}</td>
                                                            <td>{{getTextFromHtml(a.Description)}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{a.Pinned_Article}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{a.Pinned_Article}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{a.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{a.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(a,'Article')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>


                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab_content">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Notice">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="NoticeDataTable" class="table key-buttons text-md-nowrap" style="width: 100%;">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Notice Title</th>
                                                            <th class="border-bottom-0">Description</th>
                                                            <th class="border-bottom-0">Pinned Notice</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="n in NoticeData">
                                                            <td>{{n.ID}}</td>
                                                            <td>{{n.Notice_Title}}</td>
                                                            <td>{{getTextFromHtml(n.Description)}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{n.Pinned_Notice}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{n.Pinned_Notice}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{n.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{n.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(n,'Notice')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="tab_content">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Event">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="EventDataTable" class="table key-buttons text-md-nowrap" style="width: 100%;">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Event Title</th>
                                                            <th class="border-bottom-0">Start Date</th>
                                                            <th class="border-bottom-0">End Date</th>
                                                            <th class="border-bottom-0">Description</th>
                                                            <th class="border-bottom-0">Pinned Event</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="e in EventData">
                                                            <td>{{e.ID}}</td>
                                                            <td>{{e.Event_Name}}</td>
                                                            <td>{{formatDate(e.Start_Date,'MM/DD/YYYY h:mm:ss a','DD MMM YYYY')}}</td>
                                                            <td>{{formatDate(e.End_Date,'MM/DD/YYYY h:mm:ss a','DD MMM YYYY')}}</td>
                                                            <td>{{getTextFromHtml(e.Description)}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{e.Pinned_Event}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{e.Pinned_Event}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{e.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{e.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(e,'Event')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab_content">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Slider">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="SliderDataTable" class="table key-buttons text-md-nowrap" style="width: 100%;">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Slider Title</th>
                                                            <th class="border-bottom-0">Slider Subject</th>
                                                            <th class="border-bottom-0">Slider_Image_Url</th>
                                                            <th class="border-bottom-0">Description</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="s in SliderData">
                                                            <td>{{s.ID}}</td>
                                                            <td>{{s.Slider_Title}}</td>
                                                            <td>{{s.Slider_Subject}}</td>
                                                            <td><img src={{urlfix+s.Slider_Image_Url}} /></td>
                                                            <td>{{getTextFromHtml(s.Description)}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{s.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{s.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(s,'Slider')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab_content">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Award">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="AwardDataTable" class="table key-buttons text-md-nowrap" style="width: 100%;">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Award Title</th>
                                                            <th class="border-bottom-0">Emp Code</th>
                                                            <th class="border-bottom-0">Reason</th>
                                                            <th class="border-bottom-0">Pinned Awards</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="aw in AwardData">
                                                            <td>{{aw.ID}}</td>
                                                            <td>{{aw.Award_type}}</td>
                                                            <td>{{aw.Emp_Code}}</td>
                                                            <td>{{aw.Reason}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{aw.Pinned_Awards}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{aw.Pinned_Awards}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{aw.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{aw.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-primary btn-icon"><i class="far fa-edit"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab_content">
                                    <div class="d-flex flex-row justify-content-end pd-b-20">
                                        <div class="col-lg-2">
                                            <button class="btn btn-info btn-block add_Click" data-Tab="Gallery">Add</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="table-responsive">
                                                <table id="GalleryDataTable" class="table key-buttons text-md-nowrap" style="width: 100%;">
                                                    <thead>
                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Album Title</th>
                                                            <th class="border-bottom-0">Pinned Album</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="g in GalleryData">
                                                            <td>{{g.ID}}</td>
                                                            <td>{{g.Album_Title}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{g.Pinned_Album}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{g.Pinned_Album}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{g.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{g.Active}}>No</span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-icon-list">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(g,'Gallery')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab_content">
                                    <p> On the other hand, we denounce with righteous indignation and dislike men who are so beguiled and demoralized by the charms of pleasure of the moment, so blinded by desire, that they cannot foresee the pain and trouble that are bound to ensue; and equal blame belongs to those who fail in their duty through weakness of will, which is the same as saying through shrinking from toil and pain. These cases are perfectly simple and easy to distinguish. In a free hour, when our power of choice is untrammelled and when nothing prevents .</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /div -->
                </div>
            </div>
        </div>
    </div>
    <div class="modal left fade" id="exampleModalRight" tabindex="" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body" style="overflow-y: scroll; max-height:90%;">
                    <div class="row row-sm">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <label for="Gallery_Title">Gallery Title</label>
                                <input type="text" class="form-control" id="Gallery_Title" placeholder="Enter Gallery Title">
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <div class="main-toggle-group-demo yes-toggle">
                                    <label for="Reason_For_Award">Artical Pinned</label>
                                    <div class="main-toggle mg-l-20" id="pinned_Check">
                                        <span></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <div class="main-toggle-group-demo yes-toggle">
                                    <label for="Reason_For_Award">Active</label>
                                    <div class="main-toggle mg-l-20" id="active_Check">
                                        <span></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12 tabs-style-1">
                            <div class=" tab-menu-heading">
                                <div class="tabs-menu1">
                                    <!-- Tabs -->
                                    <ul class="nav panel-tabs main-nav-line">
                                        <li><a href="#tab1" class="nav-link active" data-toggle="tab">Drop Images</a></li>
                                        <li><a href="#tab2" class="nav-link" data-toggle="tab">Previously Added</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="panel-body tabs-menu-body main-content-body-right border">
                                <div class="tab-content">
                                    <div class="tab-pane active" id="tab1">
                                        <div class="form-group">
                                            <input id="demo" type="file" name="files" accept=".jpg, .png, image/jpeg, image/png, html, zip, css,js" multiple>
                                            <div class="d-flex flex-row-reverse pd-t-20">
                                                <button class="btn btn-dark" style="display:none" id="btnuploadAllButton" onclick="uploadAllButton()">Upload all files</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="tab-pane" id="tab2">
                                        <div class="row">
                                            <div class="col-md-6 col-lg-6">
                                                <div class="card">
                                                    <div class="card-body pd-8-f">
                                                        <div class="row">
                                                            <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
                                                                Test
                                                            </div>
                                                            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
                                                                <div class="d-flex flex-row justify-content-end">
                                                                    <button class="btn btn-icon-list" ng-click="editPopUp(g,'Gallery')" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"><i class="si si-pencil text-primary mr-2"></i></button>
                                                                    <i class="si si-trash text-danger mr-2" data-toggle="tooltip" title="Delete" data-placement="top" data-original-title="Delete"></i>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div><img alt="Image" class="img-fluid card-img-top" src="../assets/img/photos/1.jpg">
                                                </div>
                                            </div>
                                            <div class="col-md-6 col-lg-6">
                                                <div class="card">
                                                    <div class="card-body pd-8-f">
                                                        <div class="row">
                                                            <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
                                                                Test
                                                            </div>
                                                            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
                                                                <div class="d-flex flex-row justify-content-end">
                                                                    <i class="si si-trash text-danger mr-2" data-toggle="tooltip" title="Delete" data-placement="top" data-original-title="Delete"></i>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div><img alt="Image" class="img-fluid card-img-top" src="../assets/img/photos/1.jpg">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" id="saveActionData" data-saveType="" data-operation="" data-updateId="">Save</button>
                    <!--<button type="button" class="btn btn-primary" data-dismiss="modal">Save</button>-->
                    <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Modal alert message Success -->
    <div class="modal" id="SuccessModelProject" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content tx-size-sm">
                <div class="modal-body tx-center pd-y-20 pd-x-20">
                    <i class="icon ion-ios-checkmark-circle-outline tx-100 tx-success lh-1 mg-t-20 d-inline-block"></i>
                    <h4 class="tx-success tx-semibold mg-b-20">Congratulations!</h4>
                    <p class="mg-b-20 mg-x-20">Your request have been successfully submitted.</p><button aria-label="Close" class="btn ripple btn-success pd-x-25" data-dismiss="modal" type="button" ng-click="SaveRedirect()">Continue</button>
                </div>
            </div>
        </div>
    </div>

    <div id="stack3" class="modal hide fade" tabindex="-1" data-focus-on="input:first">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3>Stack Three</h3>
        </div>
        <div class="modal-body">
            <p>One fine body…</p>
            <input type="text" data-tabindex="1">
            <input type="text" data-tabindex="2">
        </div>
        <div class="modal-footer">
            <button type="button" data-dismiss="modal" class="btn">Close</button>
            <button type="button" class="btn btn-primary">Ok</button>
        </div>
    </div>

</div>
@section scripts
{

    <!--Internal Fileuploads js-->
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fileuploads/js/fileupload.js"></script>
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fileuploads/js/file-upload.js"></script>

    <!--Internal Fancy uploader js-->
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fancyuploder/jquery.ui.widget.js"></script>
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fancyuploder/jquery.fileupload.js"></script>
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fancyuploder/jquery.iframe-transport.js"></script>
    <script src="~/Scripts/Angular/Intranet/JS/IntranetJs/fancyuploder/jquery.fancy-fileupload.js"></script>




    <!--- Tabs JS-->
    <script src="~/assets/plugins/tabs/jquery.multipurpose_tabcontent.js"></script>
    <script src="~/assets/js/tabs.js"></script>

    <!-- Internal Select2.min js -->
    <script src="~/assets/plugins/select2/js/select2.min.js"></script>

    <script src="~/assets/plugins/sweet-alert/sweetalert.min.js"></script>

    <!--Internal  jquery-simple-datetimepicker js -->
    <script src="~/assets/plugins/amazeui-datetimepicker/js/amazeui.datetimepicker.min.js"></script>

    <!-- Internal Data tables -->
    <script src="~/assets/plugins/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/assets/plugins/datatable/js/dataTables.dataTables.min.js"></script>
    <script src="~/assets/plugins/datatable/js/dataTables.responsive.min.js"></script>
    <script src="~/assets/plugins/datatable/js/responsive.dataTables.min.js"></script>
    <script src="~/assets/plugins/datatable/js/jquery.dataTables.js"></script>
