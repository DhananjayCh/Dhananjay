                                                        <tr>
                                                            <th class="border-bottom-0">ID</th>
                                                            <th class="border-bottom-0">Article Title</th>
                                                            <th class="border-bottom-0">Description</th>
                                                            <th class="border-bottom-0">Pinned Article</th>
                                                            <th class="border-bottom-0">Active</th>
                                                            <th class="border-bottom-0">Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                        <tr ng-repeat="a in ArticleData">
                                                            <td>{{a.ID}}</td>
                                                            <td>{{a.Article_Title}}</td>
                                                            <td>{{getTextFromHtml(a.Description)}}</td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{a.Pinned_Article}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{a.Pinned_Article}}>No</span>
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-success" ng-show={{a.Active}}>Yes</span>
                                                                <span class="badge badge-danger" ng-show=!{{a.Active}}>No</span>
